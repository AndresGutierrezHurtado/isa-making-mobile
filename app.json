{
    "expo": {
        "name": "isa-making-mobile",
        "slug": "isa-making-mobile",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/isa-making-blue.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/isa-making.png",
                "backgroundColor": "#18181b"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/isa-making.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/isa-making-blue.png",
                    "imageWidth": 300,
                    "resizeMode": "contain",
                    "backgroundColor": "#18181b"
                }
            ],
            "expo-font"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
